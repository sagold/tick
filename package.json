    "eslint-plugin-promise": "4.2.1",
    "glob": "^7.1.6",
    "image-size": "^0.8.3",
    "jsdom": "^16.4.0",
    "mocha": "7.1.0",
    "node-fetch": "^2.6.0",
    "terser-webpack-plugin": "2.3.5",
    "ts-loader": "6.2.1",
